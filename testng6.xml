<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Loan Department" parallel="tests" thread-count="2">

<listeners>
    <listener class-name="testngTutorialFromRahul.listeners"></listener>
</listeners>
    <test name="Personal Loan">
        <parameter name="URL" value="PersonalLoan.com"></parameter>
        <parameter name="username" value="12345"></parameter>
        <classes>
            <class name="testngTutorialFromRahul.day4"></class>
        </classes>
    </test>
    <test name="Car Loan">
        <parameter name="URL" value="PersonalLoan.com"></parameter>
        <parameter name="username" value="12345"></parameter>
        <classes>
            <class name="testngTutorialFromRahul.day1"></class>
        </classes>
    </test>

    
</suite>